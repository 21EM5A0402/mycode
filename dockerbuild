node { 
    stage('scm checkout'){
        git 'https://github.com/narendra311777/mycode.git'
  }
  
  stage('build-image'){
     app = docker.build("narendra311777/mycode")
     }
   stage('publish-image'){
     docker.withRegistry('https://registry.hub.docker.com','Docker'){
     app.push("${env.BUILD_NUMBER}")
     app.push("latest")
}
}
}
